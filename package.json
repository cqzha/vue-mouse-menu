    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.1",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
